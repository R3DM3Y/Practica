<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Practica.DeleteWindow"
        Title="acceptDeleteWindow"
        SizeToContent="WidthAndHeight"
        WindowStartupLocation="CenterOwner">
    <StackPanel Margin="20" Spacing="15" Width="300">
        <TextBlock x:Name="MessageText" TextWrapping="Wrap" TextAlignment="Center"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Spacing="10">
            <Button Content="Удалить" Click="DeleteButton_Click" Width="80" Background="#FF0000" Foreground="White"/>
            <Button Content="Отмена" Click="CancelButton_Click" Width="80"/>
        </StackPanel>
    </StackPanel>
</Window>
